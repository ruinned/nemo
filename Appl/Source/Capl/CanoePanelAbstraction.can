/**
 * Start the timer given by timerId
 * @param timerId: Identification of the timer
 * @param _time: time in MS for the timer
 **/
void CAL_StartMSTimer(dword timerId, dword _time)
{
  switch ( timerId )
  {
    case NmCan_RxIndicationTimer_CAN_Backbone2_ID : setTimer(NmCan_RxIndicationTimer_CAN_Backbone2, _time); break;
    case NmCan_StartCyclicTransmissionTimer_CAN_Backbone2_ID : setTimer(NmCan_StartCyclicTransmissionTimer_CAN_Backbone2, _time); break;
    case SmLin_WakeupIndicationTimer_LIN1_CIOM_ID : setTimer(SmLin_WakeupIndicationTimer_LIN1_CIOM, _time); break;
    default: break;
  }
}
/**
 * Stop a timer given by timerId
 * @param timerId: Identification of the timer
 **/
void CAL_StopMSTimer(dword timerId)
{
  switch ( timerId )
  {
    case NmCan_RxIndicationTimer_CAN_Backbone2_ID : cancelTimer(NmCan_RxIndicationTimer_CAN_Backbone2); break;
    case NmCan_StartCyclicTransmissionTimer_CAN_Backbone2_ID : cancelTimer(NmCan_StartCyclicTransmissionTimer_CAN_Backbone2); break;
    case SmLin_WakeupIndicationTimer_LIN1_CIOM_ID : cancelTimer(SmLin_WakeupIndicationTimer_LIN1_CIOM); break;
    default: break;
  }
}
/**
 * Restart the timer given by timerId
 * @param timerId: Identification of the timer
 * @param _time: time in MS for the timer
 **/
void CAL_RestartMSTimer(dword timerId, dword _time)
{
  CAL_StopMSTimer(timerId);
  CAL_StartMSTimer(timerId, _time);
}



#elif TOOL_MINOR_VERSION == 1
  minorVersion = 1;
#elif TOOL_MINOR_VERSION == 2
  minorVersion = 2;
#elif TOOL_MINOR_VERSION == 3
  minorVersion = 3;
#elif TOOL_MINOR_VERSION == 4
  minorVersion = 4;
#elif TOOL_MINOR_VERSION == 5
  minorVersion = 5;
#elif TOOL_MINOR_VERSION == 6
  minorVersion = 6;

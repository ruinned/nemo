/**
 * Handle CAN messages occurrence on the channel CAN_Backbone2
 **/
on message CAN_Backbone2.*
{
  dword pduId;
  word i;
  CAL_OnFrameIndication( 0 );
  pduId = getPduIdFromCanId( this.id, 0 );
  if ( pduId != PDU_ID_CAN_BACKBONE2_EMS_BB2_05P_OBACKBONE2_401962256x && pduId != PDU_ID_CAN_BACKBONE2_HMIIOM_BB2_15P_OBACKBONE2_419229536x )
  {
    CAL_OnCanMessageIndication( 0 );
  }
  if ( pduId == GLOBAL_MAX_INVALID_DWORD_VALUE )
  {
    return;
  }
  if ( getIsRxPdu( pduId ) )
  {
    onRxPduUpdate( pduId );
  }
  else
  {
    for (i = 0; i < this.DataLength; ++i)
    {
      pduPayload[i] = this.byte(i);
    }
    onTxPduUpdate( pduId, pduPayload, this.DataLength );
  }
}
on linMessage LIN1_CIOM.*
{
  if (this.id == 0x3C && this.dlc == 8) /* MasterReq('Go-to-Sleep') */
  {
    if (this.byte(0) == 0x00 && this.byte(1) == 0xFF && this.byte(2) == 0xFF && this.byte(3) == 0xFF &&
        this.byte(4) == 0xFF && this.byte(5) == 0xFF && this.byte(6) == 0xFF && this.byte(7) == 0xFF)
    {
      return;
    }
  }
  CAL_OnFrameIndication(1);
  CAL_OnLinMessageIndication(1);
}

on linMessage LIN1_CIOM::FSP2_Frame_L1
{
  CAL_OnFrameIndication( 1 );
  CAL_OnLinMessageIndication( 1 );
  onRxPduUpdate( PDU_ID_LIN1_CIOM_FSP2_FRAME_L1 );
}

on linMessage LIN1_CIOM::FSP_1_2_Req_L1
{
  word i;
  for (i = 0; i < this.dlc; ++i)
  {
    pduPayload[i] = this.byte(i);
  }
  CAL_OnFrameIndication( 1 );
  CAL_OnLinMessageIndication( 1 );
  onTxPduUpdate( PDU_ID_LIN1_CIOM_FSP_1_2_REQ_L1, pduPayload, this.dlc );
}


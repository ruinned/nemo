/*********************************************************
 * SM LIN API
 *********************************************************/

/**
 * Initialize SM LIN internal data.
*/
void SmLin_Init()
{
  dword i;
  enum EBusType busType;
  for (i = 0; i < GLOBAL_MAX_CHANNELS; ++i)
  {
    busType = cfg_GetBusType(i);
    if (busType == E_LIN)
    {
      smLin_SetState(i, LINSM_NO_COM);
      smLin_SetIsNetworkRequested(i, false);
    }
  }
}

/**
 *
 * Start the LIN communication.
 *
 * @param channel: channel ID.
 * @param internalWakeup: true: performs LIN Wakeup sequence on channel
 **/
void SmLin_FullComRequest(dword channel, byte internalWakeup)
{
  enum LinSmBusStateType currentState;
  smLin_SetIsNetworkRequested(channel, internalWakeup);
  currentState = smLin_GetState(channel);
  switch(currentState)
  {
    case LINSM_NO_COM:
      if (internalWakeup)
      {
        smLin_ChangeLinSmStateIfAny(channel, LINSM_NO_COM_FULL_COM_PENDING);
        CAL_LinSendWakeup(channel);
      }
      else
      {
        smLin_ChangeLinSmStateIfAny(channel, LINSM_FULL_COM_NO_COM_PENDING);
      }
      break;
    case LINSM_FULL_COM_NO_COM_PENDING:
      smLin_ChangeLinSmStateIfAny(channel, LINSM_FULL_COM);
      break;
  }
}

/**
 * Stop the LIN communication.
 * @param channel: channel ID.
 **/
void SmLin_NoComRequest(dword channel)
{
  enum LinSmBusStateType currentState;
  currentState = smLin_GetState(channel);
  switch(currentState)
  {
    case LINSM_FULL_COM:
      smLin_ChangeLinSmStateIfAny(channel, LINSM_FULL_COM_NO_COM_PENDING);
      break;
    case LINSM_NO_COM_FULL_COM_PENDING:
      smLin_ChangeLinSmStateIfAny(channel, LINSM_NO_COM);
      CAL_EnableBusActivityDetection(channel);
      smLin_StopCyclicWakeupTimer(channel);
      break;
  }
}



/*********************************************************
 * SM LIN callbacks
 *********************************************************/


/**
 * Handle a bus state change in case of NormalOperation.
 * @param channel: channel ID.
 **/
void SmLin_BusNormalOperationIndication(dword channel)
{
  byte isNetworkRequested;
  enum LinSmBusStateType currentState;
  isNetworkRequested = smLin_GetIsNetworkRequested(channel);
  currentState = smLin_GetState(channel);
  switch(currentState)
  {
    case LINSM_NO_COM_FULL_COM_PENDING:
      if (isNetworkRequested)
      {
        smLin_ChangeLinSmStateIfAny(channel, LINSM_FULL_COM);
        smLin_StopCyclicWakeupTimer(channel);
      }
      break;
  }
}

/**
 * Handle a bus state change in case of BusSleep.
 * If FullCommunication is still requested, the bus is woken up again.
 * @param channel: channel ID.
 **/
void SmLin_BusSleepIndication(dword channel)
{
  byte isNetworkRequested;
  enum LinSmBusStateType currentState;
  isNetworkRequested = smLin_GetIsNetworkRequested(channel);
  currentState = smLin_GetState(channel);
  switch(currentState)
  {
    case LINSM_FULL_COM_NO_COM_PENDING:
      smLin_ChangeLinSmStateIfAny(channel, LINSM_NO_COM);
      break;
    case LINSM_FULL_COM:
      smLin_ChangeLinSmStateIfAny(channel, LINSM_NO_COM_FULL_COM_PENDING);
      if (isNetworkRequested)
      {
        CAL_DisableBusActivityDetection(channel);
        CAL_LinSendWakeup(channel);
      }
      break;
  }
}

/**
 * Handle transmission of a cyclic LIN wakeup in case of BusSleep.
 * @param channel: channel ID.
 **/
void SmLin_WakeupIndicationTimeout(dword channel)
{
  CAL_LinSendWakeup(channel);
  smLin_RestartCyclicWakeupTimer(channel);
}

/*********************************************************
 * LINSM internal functions
 *********************************************************/

/**
 * Handle a LINSM state change and informs the state manager about the state change.
 * If the LINSM enters NO_COM_FULL_COM_PENDING, the timer for sending a LIN wakeup is restarted.
 * @param channel: channel ID.
 * @param state:   New LINSM state to enter.
 **/
void smLin_ChangeLinSmStateIfAny(dword channel, enum LinSmBusStateType state)
{
  enum ComMComModeType prevComMode;
  enum LinSmBusStateType prevState;
  prevState = smLin_GetState(channel);
  if (state != prevState)
  {
    prevComMode = comM_GetComModeState(channel);
    smLin_SetState(channel, state);
    if (smLin_GetCurrentComMode(channel) != prevComMode)
    {
      Sm_ModeIndication(channel, smLin_GetCurrentComMode(channel));
    }
    if (LINSM_NO_COM_FULL_COM_PENDING == state)
    {
      smLin_RestartCyclicWakeupTimer(channel);
    }
  }
}

/**
 * Get the current ComM mode corresponding to the current LINSM state
 * @param channel: channel ID.
 **/
enum ComMComModeType smLin_GetCurrentComMode(dword channel)
{
  switch ( smLin_GetState(channel) )
  {
    case LINSM_UNINIT: return COMM_UNINIT;
    case LINSM_NO_COM:
    case LINSM_NO_COM_FULL_COM_PENDING: return COMM_NO_COMMUNICATION;
    case LINSM_FULL_COM:
    case LINSM_FULL_COM_NO_COM_PENDING: return COMM_FULL_COMMUNICATION;
    default: return COMM_UNINIT;
  }
}

/**
 * Restart the cyclic wakeup timer
 * @param channel: channel ID.
 **/
void smLin_RestartCyclicWakeupTimer(dword channel)
{
  CAL_RestartMSTimer(smLinCfg_GetSendWakeupTimerId(channel), LIN_SEND_WAKEUP_CYCLE_TIME);
}

/**
 * Stop the cyclic wakeup timer
 * @param channel: channel ID.
 **/
void smLin_StopCyclicWakeupTimer(dword channel)
{
  CAL_StopMSTimer(smLinCfg_GetSendWakeupTimerId(channel));
  if(LINSM_NO_COM_FULL_COM_PENDING == smLin_GetState(channel))
  {
    CAL_StartMSTimer(smLinCfg_GetSendWakeupTimerId(channel), LIN_SEND_WAKEUP_CYCLE_TIME);
  }
}


/**
 * The value of the system variable NmEcuNetworkRequest changed, e.g. because the ECU NW Request or Release button was pressed.
 * Request or release the current channel depending on the new value of the system variable.
 **/
on sysvar_update StartApplication::NmEcuNetworkRequest
{
  if(@this != PANEL_SWITCH_VALUE_PRESSED)
  {
    startRxCmdSignalConfTimer(0);
    nmUc_DutNetworkRequest(@sysvar::StartApplication::NmCurrentChannel,@this);
  }
}

/**
 * The value of the system variable NmEcuTimerNetworkRequestState changed, e.g. when new ECU Network Request state from timer is detected.
 * NmEcuTimerNetworkRequestCounter count up when NmEcuTimerNetworkRequestState equals 1.
 **/
on sysvar StartApplication::NmEcuTimerNetworkRequestState
{
  if(@this == 1)
  {
    @sysvar::StartApplication::NmEcuTimerNetworkRequestCounter++;
  }
}

/**
 * The value of the system variable NmEcuTimerNetworkRequestState changed, e.g.  e.g. when new ECU Network Request state from user is detected.
 * NmEcuTimerNetworkRequestCounter count up when NmEcuUserNetworkRequestState equals 1.
 **/
on sysvar StartApplication::NmEcuUserNetworkRequestState
{
  if(@this == 1)
  {
    @sysvar::StartApplication::NmEcuUserNetworkRequestCounter++;
  }
}

/**
 * The value of the system variable NmTesterNetworkRequest changed, e.g. because the Tester NW request switch was pressed.
 * Request or release the current channel depending on the new value of the system variable.
 **/
on sysvar_update StartApplication::NmTesterNetworkRequest
{
  if (@this == 1)
  {
    ComM_NetworkRequest(@sysvar::StartApplication::NmCurrentChannel);
  }
  else
  {
    ComM_NetworkRelease(@sysvar::StartApplication::NmCurrentChannel);
  }
}


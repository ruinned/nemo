/**
 * In the COM use case, calculate the new signal values by scaling the current sensor value
 * and trigger the transmission of the corresponding messages to the ECU.
 **/
void sendComRxSignals(byte sensorValue)
{
  int64 scaledValue;
  dword signalLength;
  scaledValue = sensorValue*0x100;                                /* Scale uint8 [0 ... 0xff] to uint16 [0 ... 0xffff] */
  updateSignal(SIGNAL_ID_CAN_BACKBONE2_EMS_BB2_05P_OBACKBONE2_ENGINEGASRATE_OEMS_BB2_05P_OBACKBONE2, scaledValue);
  scaledValue = sensorValue;                                      /* Scale uint8 [0 ... 0xff] to uint8 [0 ... 0xff] */
  updateSignal(SIGNAL_ID_LIN1_CIOM_FSP2_FRAME_L1_FSP2SWITCHSTATUSL1, scaledValue);
  outputPdu(PDU_ID_CAN_BACKBONE2_EMS_BB2_05P_OBACKBONE2_401962256x );
  outputPdu(PDU_ID_LIN1_CIOM_FSP2_FRAME_L1 );
}


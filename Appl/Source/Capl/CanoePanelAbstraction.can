/**
 * Write NM date into the payload
 **/
void nm_ConfigureNmPayload(byte payload[],
  word length,
  byte isPnEnabled,
  byte cbvPosInPayload,
  byte isNvVoted,
  byte pnInfoOffset,
  byte pnInfoLength)
{
  qword pnInfoMask = PNC_INFO_BYTE_MASK;
  word i;
  byte byteShift;
  for (i=0; i<length; ++i)
  {
    payload [i] = 0;
  }
  if (cbvPosInPayload >= length)
  {
    return;
  }
  if (pnInfoOffset + pnInfoLength > length)
  {
    return;
  }

  if (isPnEnabled)
  {
    payload[cbvPosInPayload] |= 0x40; // 6-bit is set
    byteShift = 0;
    for (i = pnInfoOffset; i < pnInfoOffset + pnInfoLength; ++i)
    {
      payload[i] = (pnInfoMask >> byteShift) & 0xFF;
      byteShift += 8;
    }
  }
  if (isNvVoted)
  {
    payload[cbvPosInPayload] |= 0x80; // 7-bit is set
  }
}



